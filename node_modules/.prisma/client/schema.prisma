generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model USERINFO {
  id           Int    @id(map: "PK_NGUOIDUNG") @default(autoincrement()) @map("U_ID")
  username     String @map("USERNAME") @db.VarChar(255)
  passwordHash String @map("PWord") @db.VarChar(255)
}
